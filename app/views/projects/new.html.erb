<h1>Project</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@project) do |f| %>

        <%= f.label :name %>
        <%= f.text_field :name %>

        <%= f.label :taks_name %>
        <%= f.text_field :task_name %>

        <%= f.submit "Create new project", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>